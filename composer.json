{
    "name": "expanse/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/expanse/api-guard",
    "authors": [
        {
            "name": "Glen Solsberry",
            "email": "gms8994@gmail.com"
        },
        {
            "name": "Chris Bautista",
            "email": "chrisbjr@gmail.com"
        },
        {
            "name": "Mark Jayson Fuentes",
            "email": "awkwardusername@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.3.0 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^5.3.0 || ^6.0 || ^7.0 || ^8.0",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^8.0",
        "orchestra/testbench": "^4.0",
        "phpstan/phpstan": "^0.12.56"
    },
    "autoload": {
        "psr-4": {
